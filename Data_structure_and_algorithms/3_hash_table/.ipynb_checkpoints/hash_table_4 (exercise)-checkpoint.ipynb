    "month_temp = []\n",
    "with open('nyc_weather.csv','r') as f:\n",
    "    for line in f:\n",
    "        token = line.split(',')\n",
    "        date = token[0]\n",
    "        temp = token[1]\n",
    "        month_temp.append([date,temp])\n",
    "month_temp"
   ]
